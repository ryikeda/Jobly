      "maxLength": 50,
      "examples": ["manager"]
    },
    "salary": {
      "$id": "#/properties/salary",
      "type": "number",
      "title": "The salary schema",
      "default": 0,
      "examples": [135000]
    },
    "equity": {
      "$id": "#/properties/equity",
      "type": "number",
      "title": "The equity schema",
      "default": 0.0,
      "examples": [0.05]
    },
    "company_handle": {
      "$id": "#/properties/company_handle",
      "type": "string",
      "title": "The company_handle schema",
      "default": "",
      "minLength": 1,
      "maxLength": 50,
      "examples": ["apple"]
    }
  },
  "additionalProperties": false
}
