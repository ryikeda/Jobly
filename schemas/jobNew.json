{
  "$id": "http://example.com/example.json",
  "type": "object",
  "examples": [
    {
      "title": "manager",
      "salary": 135000,
      "equity": 0.05,
      "company_handle": "apple"
    }
  ],
  "required": ["title", "salary", "equity", "company_handle"],
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The title schema",
      "default": "",
