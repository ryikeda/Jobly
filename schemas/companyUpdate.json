{
  "$id": "http://example.com/example.json",
  "type": "object",
  "examples": [
    {
      "handle": "TSLA",
      "name": "Tesla",
      "num_employees": "500",
      "description": "electric cars maker",
      "logo_url": "logo.com/tesla"
    }
