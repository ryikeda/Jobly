{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "default": {},
  "examples": [
    {
      "username": "guest",
      "password": "password",
      "first_name": "guest",
      "last_name": "Gestlast",
      "email": "guest@email.com",
      "photo_url": "myphoto.com"
    }
  ],
  "required": ["username", "password", "first_name", "last_name", "email"],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The username schema",
      "default": "",
      "minLength": 1,
      "maxLength": 50,
      "examples": ["guest"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The password schema",
      "default": "",
      "minLength": 6,
      "maxLength": 50,
      "examples": ["password"]
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The first_name schema",
      "minLength": 1,
      "maxLength": 50,
      "default": "",
      "examples": ["guest"]
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The last_name schema",
      "minLength": 1,
      "maxLength": 50,
      "default": "",
      "examples": ["Gestlast"]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The email schema",
      "format": "email",
      "default": "",
      "examples": ["guest@email.com"]
    },
    "photo_url": {
      "$id": "#/properties/photo_url",
      "type": "string",
      "title": "The photo_url schema",
      "format": "uri",
      "default": "",
      "examples": ["myphoto.com"]
    }
  },
  "additionalProperties": false
}
